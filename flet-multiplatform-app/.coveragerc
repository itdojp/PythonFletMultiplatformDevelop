[run]
branch = True

[report]
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.
    pass
    raise ImportError
    raise ValueError
    raise Exception
    except:
    except Exception:
    except:  # noqa
    except Exception:  # noqa
fail_under = 70
show_missing = True
skip_covered = True

[html]
directory = htmlcov
title = Test Coverage Report
